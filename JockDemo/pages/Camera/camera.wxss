.container{
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  background-color: #f1f1f1;
   position: relative;
   height: 1100rpx;
}
.home-top {
  /* border: 1px solid red; */
}
.home-top text{
  display: block;
  text-align: center;
  margin-top: 100rpx;
  /* border: 1px solid red; */
}
.home-top text:first-child{
  width: 600rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: rgb(131, 147, 180);
  z-index: 99;
  border-radius: 20rpx;
  margin-top: -100rpx;
  color: #fff;
  /* border: 1px solid red; */
}
.home-container{
  display: flex;
  /* border: 1px solid red; */
  flex-direction: column
}
.btn{
  width: 365rpx;
  height: 100rpx;
  border-radius: 20rpx;
  background-color: rgb(131, 147, 180);
  color: #fff;
  font-size: 40rpx;
}


.list-msg {
    padding: 0 20rpx;
    background-color: #fff;
    position:relative;
    top: -600rpx;
}
 
.list-msg1 {
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
 
.list-msg .list-msg2 {
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border: 1px solid #ccc;
    padding: 0 10rpx;
}
 
.select_box {
    background-color: #eee;
    padding: 0 10rpx;
    width: 93%;
    z-index: 1;
    overflow: hidden;
    animation: myfirst 0.5s;
}
 
@keyframes myfirst {
    from {
        height: 0rpx;
    }
 
    to {
        height: 210rpx;
    }
}
 
.select_one {
    height: 60rpx;
    line-height: 60rpx;
    border-bottom: 1px solid #ccc;
}

